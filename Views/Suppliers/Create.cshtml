@model Tsql3s2b.Models.Supplier
@{
	ViewData["Title"] = "Novi dobavljac";
}
<h3>Dodaj novog dobavljaca</h3>

<form asp-action="Create" method="post">
	<div class="form-group">
		<label asp-for="Companyname">Companyname:</label>
		<input type="text" asp-for="Companyname" class="form-control" />
	</div>

	<div class="form-group">
		<label asp-for="Contactname">Contactname:</label>
		<input type="text" asp-for="Contactname" class="form-control" />
	</div>


	<div class="form-group">
		<label asp-for="Contacttitle">Contacttitle:</label>
		<input type="text" asp-for="Contacttitle" class="form-control" />
	</div>


	<div class="form-group">
		<label asp-for="Address">Address:</label>
		<input type="text" asp-for="Address" class="form-control" />
	</div>



	<div class="form-group">
		<label asp-for="City">City:</label>
		<input type="text" asp-for="City" class="form-control" />
	</div>

	<div class="form-group">
		<label asp-for="Region">Region:</label>
		<input type="text" asp-for="Region" class="form-control" />
	</div>

	<div class="form-group">
		<label asp-for="Postalcode">Postalcode:</label>
		<input type="text" asp-for="Postalcode" class="form-control" />
	</div>

	<div class="form-group">
		<label asp-for="Country">Country:</label>
		<input type="text" asp-for="Country" class="form-control" />
	</div>

	<div class="form-group">
		<label asp-for="Phone">Phone:</label>
		<input type="text" asp-for="Phone" class="form-control" />
	</div>

	<div class="form-group">
		<label asp-for="Fax">Fax:</label>
		<input type="text" asp-for="Fax" class="form-control" />
	</div>

	<button type="submit" class="btn btn-success">Dodaj</button>
</form>

